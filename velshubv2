local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")

local function sendNotification()
    local dateTime = os.date("%Y-%m-%d %H:%M:%S")
    StarterGui:SetCore("SendNotification", {
        Title = "Welcome to Velozi's Hub!",
        Text = "Current Date and Time: " .. dateTime,
        Icon = "rbxassetid://115472008324688"
    })
end

sendNotification()

local DiscordLib =
    loadstring(game:HttpGet "https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/discord")()

local win = DiscordLib:Window("vel's hub :3")

local serv = win:Server("Modern Crossroads", "")

local btns = serv:Channel("Buttons")

btns:Button(
    "Infinite Yield",
    function()
        DiscordLib:Notification("Notification", "Ran infinite yield!", "Okay!")
     loadstring(game:HttpGet "https://raw.githubusercontent.com/edgeiy/infiniteyield/master/source")()
    end
)

btns:Button(
    "Hitbox Extender",
    function()
        DiscordLib:Notification("Notification", "Ran hitbox extender!", "Okay!")
     loadstring(game:HttpGet "https://raw.githubusercontent.com/velozii/hbe/refs/heads/main/hbe")()
    end
)

btns:Button(
    "Check for Supply Drop",
    function()
        DiscordLib:Notification("Notification", "Checking for airdrop..", "Okay!")
     loadstring(game:HttpGet "https://raw.githubusercontent.com/velozii/hbe/refs/heads/main/SDNotif")()
    end
)

btns:Label("If you don't get a notification, a airdrop has not spawned :)")




btns:Seperator()

local tgls = serv:Channel("Toggles")

tgls:Toggle(
    "test",
    false,
    function(bool)
        print(bool)
    end
)

local sldrs = serv:Channel("Sliders")

local sldr =
    sldrs:Slider(
    "test",
    0,
    1000,
    400,
    function(t)
        print(t)
    end
)

sldrs:Button(
    "test",
    function()
        sldr:Change(50)
    end
)

local drops = serv:Channel("Dropdowns")

local drop =
    drops:Dropdown(
    "test",
    {"Option 1", "Option 2", "Option 3", "Option 4", "Option 5"},
    function(bool)
        print(bool)
    end
)

drops:Button(
    "test",
    function()
        drop:Clear()
    end
)

drops:Button(
    "test",
    function()
        drop:Add("Option")
    end
)

local clrs = serv:Channel("Colorpickers")

clrs:Colorpicker(
    "ESP Color",
    Color3.fromRGB(255, 1, 1),
    function(t)
        print(t)
    end
)

local textbs = serv:Channel("Textboxes")

textbs:Textbox(
    "test",
    "Type here!",
    true,
    function(t)
        print(t)
    end
)

local lbls = serv:Channel("Labels")

lbls:Label("Seer and vel are cool! :)")

local bnds = serv:Channel("Binds")

bnds:Bind(
    "test",
    Enum.KeyCode.RightShift,
    function()
        print("test")
    end
)
win:Server("Main", "http://www.roblox.com/asset/?id=6031075938")
